appearance:
    progress_bar: '[ -=]'
key_bindings:
    play_pause: 'space'
    volume_up:
        - '='
        - '+'
    volume_down: '-'
    seek_forward:
        - '.'
        - '>'
    seek_backward:
        - ','
        - '<'
persistent:
    volume: 1
library:
    index_paths: '~/Music'
system:
    log_file: 'pyamp.log'
    log_level: 'DEBUG'
    GST_DEBUG:
    GST_DEBUG_FILE: 'gstreamer.log'
